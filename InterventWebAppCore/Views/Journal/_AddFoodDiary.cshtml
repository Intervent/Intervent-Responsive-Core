<script>

	$(document).ready(function () {
		if ("@Model.HasActivePortal" != "True")
		{
            $('.add-food-details').addClass('hide');
            $('.food-detail-delete').addClass('hide');
            $('.food-details-trigger').addClass('hide');
		}
		
        $('#food-date').fdatepicker({
            format: "@Model.DateFormat.ToLower()",
            endDate: new Date()
        });
        $('#food-date').val($('#date-diary').val());
        $('#foodDiaryDate').val($('#hdate-diary').val());

		$("#food-date").on("change", function (ev) {
            $('#date-diary').val($('#food-date').val());
			$('#hdate-diary').val(toSystemDateFormat($('#date-diary').val()));
			ListFoodDetails(ev, 'food-entry', true);
        });

        $('#add-food-form').foundation();
        $('#add-food-form').on('forminvalid.zf.abide', function () {
            var invalid_fields = $(this).find('[data-invalid]');
        }).on('formvalid.zf.abide', function (ev) {
			$('#FoodDiary_Date').val($('#foodDiaryDate').val());
			var id = $(this).parents('.food-entry-modal').attr('id');
            $.ajax({
                url: "@Url.Action("AddtoFoodDiary", "Journal")",
                data: $("#add-food-form").serialize(),
                type: 'POST',
                dataType: "json",
				success: function (data) {
					reload = true;
                    ListFoodDetails(ev, id, false);
                }
            }).fail(function (jqXHR, textStatus, errorThrown) {
                RedirectToErrorPage(jqXHR.status);
            });
            $("#loader-wrapper").fadeOut();
        }).on("submit", function (ev) {
            ev.preventDefault();
        });

        $(".show-details-trigger").on("click", function () {
            var target = $(this).data("toggle");
            if($(this).parents(".details-summary").find("#"+target).is(":visible")) {
                $(this).parents(".details-summary").find("#"+target).hide();
            }
            else {
                $(".food-dropdown-pane").hide();
                $(this).parents(".details-summary").find("#"+target).toggle();
            }
            $('#add-food-details').foundation('close');
        });

        $(".food-dropdown-pane button.hollow").on("click", function () {
			$(this).parents(".food-dropdown-pane").hide();
        });

        $("#food-date").fdatepicker().on('show', function () {
            $(".datepicker").css("position", "fixed");
        });
        $("#food-date").fdatepicker().on('hide', function () {
            $(".datepicker").css("position", "absolute");
        });

        $(".recent-food-entry-list span").on("click", function () {
            var foodName = $(this).text();
            $(".recent-food-entry-list span").removeClass("is-selected");
            $(this).addClass("is-selected");
            $("input#FoodDiary_Name").val(foodName);
        });

    }); // document ready ends

    function AddFood(foodType, mealType) {
        $(".foodGroup1, .foodGroup2, .foodGroup3, .foodGroup4, .foodGroup5, .foodGroup6").addClass("hide");
        if ($(".foodGroup" + foodType + "").length) {
            $(".recent-food-entry-list").removeClass("hide");
            $(".foodGroup" + foodType + "").removeClass("hide");
        }
        else {
            $(".recent-food-entry-list").addClass("hide");
        }
        $("#FoodDiary_MealTypeId").val(mealType);
        $("#FoodDiary_FoodGroupId").val(foodType);
    }


	function CloseModel() {
		$('.edit-cal-entry').removeClass('is-open');
        if (reload) {
            LoadFoodDetails();
            $('#food-entry').foundation('close');
        }
        else {
			$('#food-entry').foundation('close');
            RemoveAddDetails();
        }
    }

	function TabChange(e, id) {
		e.preventDefault();
		e.stopPropagation();
		if (id == "")
			tabTarget = "food-breakfast";
		else
			tabTarget = id;
		$tabLink = $('[data-tablink="' + tabTarget + '"]');
		if (!$tabLink.hasClass('is-active')) {
			$('#food-entry-tabs li').removeClass('is-active');
			$tabLink.addClass('is-active');
			$('[data-tabs-content="food-entry-tabs"] .tabs-panel').removeClass('is-active');
			$('[data-tabs-content="food-entry-tabs"] #' + tabTarget).addClass('is-active');
		}
		else {
			return;
		}
    }

</script>