@using InterventWebApp
@model InterventWebApp.FoodDiaryDetailsModel

<div class="welcome-title">
	<div class="food-entry-date">
        <label for="food-date">@Translate.Message("L4480")</label>
		@if (!Model.mousehover)
		{
            <input type="text" class="input-date" id="food-date">
		}
		else
		{
            <span class="foodDate">@Model.foodDate</span>
		}
	</div>
</div>

<div class="food-form-header">
	<div class="food-entry-tabs main-tabs">
		<ul class="tabs" id="food-entry-tabs">
			<li class="tabs-title is-active" data-tablink="food-breakfast"><a href="food-breakfast" onclick="TabChange(event, 'food-breakfast')">@Translate.Message("L3556")</a></li>
			<li class="tabs-title" data-tablink="food-snack"><a href="food-snack" onclick="TabChange(event, 'food-snack')">@Translate.Message("L3558")</a></li>
			<li class="tabs-title" data-tablink="food-lunch"><a href="food-lunch" onclick="TabChange(event, 'food-lunch')">@Translate.Message("L3557")</a></li>
			<li class="tabs-title" data-tablink="food-dinner"><a href="food-dinner" onclick="TabChange(event, 'food-dinner')">@Translate.Message("L3559")</a></li>
		</ul>
	</div>

</div>

<div class="food-form-body">
	<div class='content-box @(Model.FoodDiaryAccess == 3 ? "food-fat-only" : "") @(Model.FoodDiaryAccess >= 4 ? "food-fat-crabs" : "")'>
