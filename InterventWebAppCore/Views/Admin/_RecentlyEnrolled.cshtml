                                <img src="@program.User.Picture" alt="" />
                            </div>
                            <div class="profile-content">
                                <p><strong>@program.User.FirstName @program.User.LastName</strong></p>
                               @* <p>@program.</p>*@
                            </div>
                        </div>
                        @*<div class="card-option">
