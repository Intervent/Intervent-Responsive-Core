                                        </div>
                                        <div class="circle-graph-percents">75%</div>
                                    </div>
                                </div>
                            </div>
                        </div>*@
                    </div>
                    <!-- card header ends -->
                    @*<div class="card-body">
                        <div class="details-section-wrapper">
                            <div class="detail-section">
                                <p class="section-title">Scheduled from</p>
                                <p>@apt.StartTime</p>
                            </div>
                            <div class="detail-section">
                                <p class="section-title">Duration</p>
                                <p>@apt.Minutes Minutes</p>
                            </div>
                        </div>
                        <div class="status-tags">
                            <span class="status-tag">Last Appointment on </span>
                        </div>
                    </div>*@
                    <!-- card body ends -->
                    @*<div class="card-additional-info">
                        <p>Mark Appointment Status</p>
