@model InterventWebApp.StressGoalModel
@using InterventWebApp

<div class="grid-container">
    <div class="grid-x grid-margin-x">
        <div class="small-12 cell avoid-page-break-inside">
            <h2>@Translate.Message("L1058")</h2>
            <div class="grid-x grid-margin-x">
                <div class="small-12 cell">
                    @if (Model.stressGoalContent.Count > 0)
                    {
                        <ul class="decorated purple">
                            @for (int i = 0; i < Model.stressGoalContent.Count; i++)
                            {
                                @Html.Raw(Model.stressGoalContent[i])
                            }
                        </ul>
                    }
                </div>
            </div>
        </div>
    </div>
</div>
<a class="close-button hide-for-print" data-close>&#215;</a>