@using InterventWebApp
@model InterventWebApp.RiskChart
<script>
    @if (Model != null)
    {
        var risk = Model;
        if (risk.riskName == "PA")
        {
            @: CreateAMChart('chart-exer', @risk.currentValue, '@Html.Raw(risk.currentText)', '@risk.color1', '@Html.Raw(risk.currentText)', @risk.goalValue1, '@Html.Raw(risk.goalText1)', '@Html.Raw(risk.label)', '@risk.includeChar');
        }
        else if (risk.riskName == "Tobacco")
        {
            if (risk.startValue != null)
            {
                 @: CreateThreeChartAMChart('chart-tob', '@risk.startValue', '@Html.Raw(risk.startText)', '@risk.color3', '@risk.startYear', '@risk.currentValue', '@Html.Raw(risk.currentText)', '@risk.color1', '@risk.currentYear', '@risk.goalValue1', '@Html.Raw(risk.goalText1)', '@Html.Raw(risk.label)', '@risk.includeChar', '@Translate.Message("L3890")');
            }
            else
            {
                @: CreateAMChart('chart-tob', @risk.currentValue, '@Html.Raw(risk.currentText)', '@risk.color1', '@risk.currentYear', @risk.goalValue1, '@Html.Raw(risk.goalText1)',  '@Html.Raw(risk.label)', '@risk.includeChar');
            }
        }
        else if (risk.riskName == "Obese")
        {
            if (risk.startValue != null)
            {
                @: CreateFiveChartAMChart('chart-overweight', '@risk.goalValue0', '@Html.Raw(risk.goalText0)', '@risk.startValue', '@Html.Raw(risk.startText)', '@risk.color3', '@risk.startYear', '@risk.currentValue', '@Html.Raw(risk.currentText)', '@risk.color1', '@risk.currentYear', '@risk.goalValue1', '@Html.Raw(risk.goalText1)', '@risk.goalValue2', '@Html.Raw(risk.goalText2)',  '@Html.Raw(risk.label)', '@Translate.Message("L3890")');
            }
            else
            {
                @: CreateFourChartAMChart('chart-overweight', @risk.goalValue0, '@Html.Raw(risk.goalText0)', @risk.currentValue, '@Html.Raw(risk.currentText)', '@risk.color1', '@risk.currentYear',  @risk.goalValue1, '@Html.Raw(risk.goalText1)', @risk.goalValue2, '@Html.Raw(risk.goalText2)',  '@Html.Raw(risk.label)');
            }
        }
        else if (risk.riskName == "CAC")
        {
            @: CreateCACChart('chart-cac', @risk.currentValue, '@Html.Raw(risk.currentText)', '@Html.Raw(risk.label)');
        }
        else if (risk.riskName == "PAI")
        {
            @: CreatePAIChart('chart-pai', @risk.currentValue, '@Html.Raw(risk.currentText)', '@Html.Raw(risk.label)');
        }
        else if (risk.riskName == "ADA")
