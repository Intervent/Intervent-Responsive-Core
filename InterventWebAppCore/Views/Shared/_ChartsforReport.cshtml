            }
         }
        else if (risk.riskName == "Glucose")
        {
            if (risk.goalValue0 == 0)
            {
                if (risk.startValue != null)
                {
                    @: CreateThreeChartAMChart('chart-glucose', '@risk.startValue', '@Html.Raw(risk.startText)', '@risk.color3', '@risk.startYear', @risk.currentValue, '@Html.Raw(risk.currentText)', '@risk.color1', '@risk.currentYear', @risk.goalValue1, '@Html.Raw(risk.goalText1)', '@Html.Raw(risk.label)', '@risk.includeChar', '@Translate.Message("L3890")');
