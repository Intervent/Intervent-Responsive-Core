<script>
    Foundation.Abide.defaults.validators['validateQuestion1'] = function ($el, required, parent) {

        var group = parent.closest('.weight-group');
        var limit = group.attr('data-validator-limit').split(',');
        var countC = group.find(':checked').length;
        if (countC >= limit[0] && countC <= limit[1]) {
            group.find('#error').removeClass().addClass('form-error');
            return true;
        } else {

            group.find('#error').removeClass().addClass('form-error is-visible');
            return false;
        }
    };
    Foundation.Abide.defaults.validators['validateQuestion2'] = function ($el, required, parent) {
        var group = parent.closest('.exercise-group');
        var limit = group.attr('data-validator-limit').split(',');
        var countC = group.find(':checked').length;
        if (countC >= limit[0] && countC <= limit[1]) {
            group.find('#error').removeClass().addClass('form-error');
            return true;
        } else {
            group.find('#error').removeClass().addClass('form-error is-visible');
            return false;
        }
    }
    $(document).ready(function () {
        @if(Model.SurveyResponse != null) {
            for(var i=0; i< Model.SurveyResponse.Count; i++){
                @: var ques = @Model.SurveyResponse[i].QuestionId;
                @: var ans = @Model.SurveyResponse[i].Answer;
                @: document.getElementById("Ques_" + ques + "_option" + ans).checked = true;
            }
        }
    });

    $('#survey-form').foundation();
    $('#survey-form')
        .on('formvalid.zf.abide', function () {
                AddEditSurvey();
        })
        .on("submit", function (ev) {
            ev.preventDefault();

        })

    function AddEditSurvey() {
        var formdata = $("#survey-form").serialize();
        if (formdata != null && formdata != "") {
            var question = formdata.split("&");
        }
        else {
            return;
        }
        var question = formdata.split("&");
        var dataVal = [];
        var model = {};
        var isEligibleforIncentive = false;
        if (question.length > 0) {
            for (var i = 0; i < question.length; i++) {
                var questionId = question[i].split("=");
                if (questionId[0] == "Comments")
                    continue;
                var listData = {
                    'Id': 0,
                    'QuestionId': parseInt(questionId[0].replace("QuestionId", "")),
                    'UsersinProgramsId': 0,
                    'Answer': parseInt(questionId[1]),
                    'DateCreated': null
                };
                dataVal.push(listData);
            }
        }
        if ('@Model.SurveyResponse' == '' && dataVal.length >= 2)
            isEligibleforIncentive = true;

        model.SurveyResponse = (dataVal);
        model.EligibleForIncentive = isEligibleforIncentive;
        model.Comments = $('#Comments').val();
        var strData = JSON.stringify(model);
        $.ajax({
            url: "@Url.Action("AddEditSurvey", "Participant")",
            type: 'POST',
        dataType: "json",
        contentType: 'application/json, charset=utf-8',
        data: strData,
        success: function (data) {
            if (data != null && data.Record == true) {
                $('#satisfaction-survey').foundation('close');
                if (dataVal.length == 8) {
                    $('.myprog-followup').addClass('hide');
                }
            }
        }
    }).fail(function (jqXHR, textStatus, errorThrown) {
            RedirectToErrorPage(jqXHR.status);
        });
    }

    $('#closeSurvey').on('click', function () {
        $('#satisfaction-survey').foundation('close');
    });
</script>