                <div class="feature-info cell medium-7">
                    <h2>@Translate.Message("L535")</h2>
                    <p>@Translate.Message("L1904").</p>
                    <p>@Translate.Message("L1905").</p>
                    <p>@Translate.Message("L1906").</p>
                    <p>@string.Format(Translate.Message("L1907"), TempData.Peek("OrgContactNumber").ToString()) @string.Format(Translate.Message("L3064"), TempData["OrgContactNumber"].ToString(), TempData.Peek("OrgContactEmail").ToString())</p>
                </div>
            </div>
        </div>
    </a>
</section>

