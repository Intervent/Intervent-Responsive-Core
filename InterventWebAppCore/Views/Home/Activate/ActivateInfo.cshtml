@using InterventWebApp
@{
	ViewBag.Title = "LandingPages";
	Layout = "~/Views/Shared/_AnonymousLayout.cshtml";
	ViewBag.Details = "LandingPage";
}
<section class="feature reversed intro compass info">
    @Html.Partial("Activate/_ActivateHeader")
</section>

<!--Signup Modal-->
<div id="sign-up-modal" class="reveal small" data-reveal data-close-on-click="false" data-close-on-esc="false">
    <div class="cell medium-12">
        <h2 class="text-center">@Translate.Message("L431")</h2>
        <div data-alert id="signup-error" class="alert-box alert hide">
            Sign Up Error
            <a href="#" class="close">&times;</a>
        </div>
    </div>
    @Html.Partial("_ActivateSignUp")
    <div class="cell text-center">
        <a href="#" onclick="$('#login-modal').foundation('open'); return false;">@Translate.Message("L426")</a>
    </div>
    <button class="close-button" data-close aria-label="Close modal" type="button">
        <span aria-hidden="true">&times;</span>
    </button>
</div>

<section class="container feature">
    <a name="employerrewards" class="compass anchor">
        <div class="grid-container">
            <div class="grid-x grid-margin-x">
                <div class="feature-icon cell medium-5 medium-order-2 text-center">
                    <img src="../images/penta-logo.png" alt="" />
                </div>
                <div class="feature-info cell medium-7 medium-order-1">
                    <h2>@Translate.Message("L1348")</h2>
                    <p>@Translate.Message("L3054")</p>
                    <p>
                        @Html.Raw(@Translate.Message("L3055"))
                        <br />•	@Translate.Message("L3056")
                        <br />•	@Translate.Message("L3057")
                        <br />•	@Translate.Message("L3058")
                    </p>
                    <br/><p>@Translate.Message("L3059")</p>
                    <p>
                        @Html.Raw(@Translate.Message("L3060"))
                        <br />1.	@Html.Raw(Translate.Message("L3061"))
                        <br />2.	@Html.Raw(Translate.Message("L3062"))
                        <br />3. @Html.Raw(Translate.Message("L3063"))
                    </p>
                    <div class="small-12 cell text-center">
                        <p class="more-link" href="#" onclick="$('#incentives').foundation('open'); return false;">
                            @Translate.Message("L1363")<span class="more"></span>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </a>
</section>
<section class="container feature">
    <a name="healthassessment" class="compass anchor">
        <div class="grid-container">
            <div class="grid-x grid-margin-x">
